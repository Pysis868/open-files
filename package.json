{
  "name": "open-files",
  "main": "./lib/open-files",
  "version": "0.8.3",
  "description": "This package adds an 'open files' pane above Atom's tree-view, mimicking the functionality of Sublime Text",
  "keywords": [
    "tree-view",
    "files",
    "open"
  ],
  "repository": "https://github.com/oriolmirosa/open-files",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "consumedServices": {
    "file-icons.element-icons": {
      "versions": {
        "1.0.0": "consumeElementIcons"
      }
    }
  },
  "dependencies": {
    "atom-utils": "^0.9.2",
    "jquery": "^3.1.1",
    "cuid": "^1.3.8",
    "lodash": ">=4.17.5"
  },
  "devDependencies": {
    "pretty-format": "^4.2.1"
  }
}
